{
  "name": "@cupsadarius/cubejs-client-svelte",
  "version": "0.1.0",
  "description": "Svelte 5 client library for CubeJS analytics",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./charts/chartjs": {
      "types": "./dist/charts/adapters/chartjs.d.ts",
      "default": "./dist/charts/adapters/chartjs.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepare": "npm run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint .",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "peerDependencies": {
    "@cubejs-client/core": ">=0.35.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@cubejs-client/core": "^0.36.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@testing-library/svelte": "^5.0.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "jsdom": "^28.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "cubejs",
    "cube",
    "analytics",
    "bi",
    "data",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cupsadarius/cubejs-client-svelte.git"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
